{
  "name": "grinder-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && ts-node build-script.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "export NODE_ENV=development && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "export NODE_ENV=test && npm run seed:test && jest --runInBand",
    "test:local": "export NODE_ENV=test.local && jest --runInBand",
    "test:watch": "export NODE_ENV=test && npm run seed:test && jest --watch",
    "test:watch-local": "export NODE_ENV=test.local && npm run seed:test-local && jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:create": "sequelize-cli migration:create --name",
    "seed:create": "sequelize-cli seed:create --name",
    "migrate:dev": "ts-node ./node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "ts-node ./node_modules/.bin/sequelize db:migrate:undo",
    "seed:dev": "export NODE_ENV=development && ts-node ./node_modules/.bin/sequelize db:seed:all",
    "seed:test": "export NODE_ENV=test && ts-node ./node_modules/.bin/sequelize db:seed:all",
    "seed:test-local": "export NODE_ENV=test.local && ts-node ./node_modules/.bin/sequelize db:seed:all",
    "seed:undo": "ts-node ./node_modules/.bin/sequelize db:seed:undo"
  },
  "dependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/common": "10.3.3",
    "@nestjs/core": "^10.3.3",
    "@nestjs/microservices": "^10.3.6",
    "@nestjs/platform-express": "^10.3.6",
    "@nestjs/sequelize": "^10.0.1",
    "@nestjs/typeorm": "^10.0.2",
    "ajv": "^8.12.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "pg": "^8.11.3",
    "postgres": "^3.4.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.37.1",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.6",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.3",
    "prettier": "^2.3.2",
    "sequelize-cli": "^6.6.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.4",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  }
}
