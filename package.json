{
  "name": "grinder-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && ts-node build-script.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "export NODE_ENV=production && nest start",
    "start:dev": "export NODE_ENV=development && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "export NODE_ENV=test && jest --runInBand",
    "test:local": "export NODE_ENV=test.local && jest --runInBand",
    "test:watch": "export NODE_ENV=test && npm run seed:test && jest --watch",
    "test:watch-local": "export NODE_ENV=test.local && npm run seed:test-local && jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:create": "sequelize-cli migration:create --name",
    "seed:create": "sequelize-cli seed:create --name",
    "migrate:dev": "ts-node ./node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "ts-node ./node_modules/.bin/sequelize db:migrate:undo",
    "seed": "export NODE_ENV=production && ts-node ./node_modules/.bin/sequelize db:seed:all",
    "seed:dev": "export NODE_ENV=development && ts-node ./node_modules/.bin/sequelize db:seed:all",
    "seed:test": "export NODE_ENV=test && ts-node ./node_modules/.bin/sequelize db:seed:all",
    "seed:test-local": "export NODE_ENV=test.local && ts-node ./node_modules/.bin/sequelize db:seed:all",
    "seed:undo": "ts-node ./node_modules/.bin/sequelize db:seed:undo"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.451.0",
    "@aws-sdk/s3-request-presigner": "^3.451.0",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/common": "^10.3.2",
    "@nestjs/core": "^10.3.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.6",
    "@nestjs/microservices": "^10.3.2",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.2",
    "@nestjs/platform-socket.io": "^10.3.2",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/sequelize": "^10.0.1",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/websockets": "^10.3.2",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "ajv": "^8.13.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.5",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ioredis": "^5.5.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "postgres": "^3.4.3",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.1",
    "sequelize-typescript": "^2.0.0",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.2.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^18.18.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "aws-sdk": "^2.1691.0",
    "aws-sdk-client-mock": "^4.0.2",
    "aws-sdk-mock": "^6.2.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.2.2",
    "prettier": "^3.0.0",
    "sequelize-cli": "^6.6.2",
    "socket.io": "^4.7.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.3.3"
  }
}
